<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.test.Models.InterestsEntity" table="interests" schema="dreamloudPROD">
        <id name="intrId">
            <column name="INTR_ID" sql-type="int(11)"/>
        </id>
        <property name="intrName">
            <column name="INTR_NAME" sql-type="varchar(40)" length="40"/>
        </property>
    </class>
</hibernate-mapping>