<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.test.Models.DreammemsEntity" table="dreammems" schema="dreamloudPROD">
        <composite-id mapped="true" class="com.test.Models.DreammemsEntityPK">
            <key-property name="drmId">
                <column name="DRM_ID" sql-type="int(11)"/>
            </key-property>
            <key-property name="acctId">
                <column name="ACCT_ID" sql-type="int(11)"/>
            </key-property>
        </composite-id>
        <property name="drmmemIsAdmin">
            <column name="DRMMEM_IS_ADMIN" sql-type="tinyint(1)"/>
        </property>
    </class>
</hibernate-mapping>