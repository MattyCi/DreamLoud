<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.test.Models.DreamersEntity" table="dreamers" schema="dreamloudPROD">
        <composite-id mapped="true" class="com.test.Models.DreamersEntityPK">
            <key-property name="acctId">
                <column name="ACCT_ID" sql-type="int(11)"/>
            </key-property>
            <key-property name="acctId2">
                <column name="ACCT_ID_2" sql-type="int(11)"/>
            </key-property>
        </composite-id>
        <property name="drmrReqDatetime">
            <column name="DRMR_REQ_DATETIME" sql-type="datetime"/>
        </property>
        <property name="drmrReqMsg">
            <column name="DRMR_REQ_MSG" sql-type="varchar(250)" length="250" not-null="true"/>
        </property>
        <property name="drmrReqAppr">
            <column name="DRMR_REQ_APPR" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>